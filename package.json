{
  "name": "angular-fe",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --disableHostCheck true",
    "build": "ng build --output-path dist",
    "test": "jest --config jest.config.js",
    "test:watch": "jest --watch",
    "test:ci": "export JEST_JUNIT_OUTPUT='./reports/unit/junit-report.xml' && jest --ci --coverage",
    "lint": "ng lint",
    "dev-server": "node server",
    "e2e": "ng e2e",
    "e2e:ci:remote": "npm run e2e:ng -- --base-url http://${E2E_TEST_ROUTE}/  --dev-server-target=",
    "e2e:ci": "npm-run-all -p -r dev-server e2e:ng",
    "e2e:local": "npm-run-all -p -r dev-server e2e:ng-local",
    "e2e:system": "ng e2e --base-url http://${E2E_TEST_ROUTE}/ --protractor-config e2e/protractor.conf.system.js  --dev-server-target=",
    "e2e:ng": "ng e2e --protractor-config e2e/protractor.conf.ci.js",
    "e2e:ng-local": "ng e2e --protractor-config e2e/protractor.conf.ci.js --base-url http://localhost:4200 --dev-server-target= ",
    "clean": "rm -rf reports dist package-contents*",
    "package": "mkdir -p package-contents && cp -vr dist Dockerfile package-contents && zip -r package-contents.zip package-contents",
    "prepare-nexus": "curl -X POST  -u ${NEXUS_CREDS} -H 'Content-Type: application/json' -H 'Accept: application/json' -d @nexus.json http://${NEXUS_SERVICE_HOST}:${NEXUS_SERVICE_PORT}/service/siesta/rest/v1/script && curl -X POST  -u ${NEXUS_CREDS} -H 'Content-Type: text/plain' -H 'Accept: application/json' http://${NEXUS_SERVICE_HOST}:${NEXUS_SERVICE_PORT}/service/siesta/rest/v1/script/labs-static/run",
    "publish": "curl -vvv -u ${NEXUS_CREDS} --upload-file package-contents.zip http://${NEXUS_SERVICE_HOST}:${NEXUS_SERVICE_PORT}/repository/labs-static/com/redhat/fe/${JOB_NAME}.${BUILD_NUMBER}/package-contents.zip",
    "publish:get": "curl -vvv http://${NEXUS_CREDS}@${NEXUS_SERVICE_HOST}:${NEXUS_SERVICE_PORT}/repository/labs-static/com/redhat/fe/${BUILD_TAG}/package-contents.zip -o package-contents.zip"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.4",
    "@angular/common": "^7.1.4",
    "@angular/compiler": "^7.1.4",
    "@angular/core": "^7.1.4",
    "@angular/forms": "^7.1.4",
    "@angular/http": "^7.1.4",
    "@angular/platform-browser": "^7.1.4",
    "@angular/platform-browser-dynamic": "^7.1.4",
    "@angular/router": "^7.1.4",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-builders/jest": "^1.2.3",
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "^7.1.4",
    "@angular/language-service": "^7.1.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jest": "^23.3.2",
    "@types/node": "~8.9.4",
    "body-parser": "^1.18.3",
    "codelyzer": "~4.3.0",
    "express": "^4.16.4",
    "fs-extra": "^7.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-reporters": "^2.3.2",
    "jasmine-spec-reporter": "^4.2.1",
    "jest": "^23.6.0",
    "jest-junit": "^5.1.0",
    "jest-preset-angular": "^6.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "lokijs": "^1.5.5",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.0",
    "protractor-html-reporter-2": "^1.0.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
